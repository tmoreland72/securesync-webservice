{
   "name": "securesync_webservice",
   "version": "0.0.1",
   "description": "WSAPI to support requests from SecureSync Agents",
   "main": "build/src/index.js",
   "author": "Troy Moreland <troy@yunibas.com>",
   "license": "UNLICENSED",
   "private": true,
   "scripts": {
      "start": "functions-framework --source=build/src/ --target=securesync_webservice",
      "start:local": "env-cmd -r ./.env.json functions-framework --source=build/src --target=securesync_webservice",
      "watch": "yaml2json .env.local.yml > .env.json && concurrently \"tsc -w\" \"nodemon -w ./build/ -x npm run start:local\"",
      "lint": "gts lint",
      "clean": "gts clean",
      "compile": "tsc",
      "fix": "gts fix",
      "pretest": "yarn run compile",
      "posttest": "yarn run lint",
      "test": "jest",
      "deploy": "gcloud functions deploy securesync-webservice --gen2 --env-vars-file=.env.production.yml --runtime=nodejs16 --region=us-central1 --trigger-http --allow-unauthenticated --entry-point=securesync_webservice --service-account=yunibas-cloud-function-dev@acoustic-result-354001.iam.gserviceaccount.com",
      "logs": "gcloud alpha logging tail"
   },
   "dependencies": {
      "@google-cloud/functions-framework": "^3.1.2",
      "@yunibas/adapters": "^1.1.6",
      "axios": "^1.1.3",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "helmet": "^6.0.0",
      "hpp": "^0.2.3",
      "jsonschema": "^1.4.1",
      "short-uuid": "^4.2.0"
   },
   "devDependencies": {
      "@types/compression": "^1.7.2",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/hpp": "^0.2.2",
      "@types/jest": "^29.1.2",
      "@types/jsonschema": "^1.1.1",
      "@types/node": "^14.11.2",
      "@types/supertest": "^2.0.12",
      "concurrently": "^7.4.0",
      "env-cmd": "^10.1.0",
      "gts": "^3.1.1",
      "jest": "^29.2.0",
      "supertest": "^6.2.4",
      "ts-jest": "^29.0.3",
      "ts-node": "^10.9.1",
      "typescript": "^4.8.4"
   }
}
